#!/usr/bin/env python3
# -*- python -*-

import os
import sys

wd = os.path.realpath(os.path.dirname(__file__))
sys.path = [wd + '/../../bottle', wd + '/src', wd + 'lib/python'] + sys.pat

import bottle
import infinit.oracles.meta
bottle.run(app = infinit.oracles.meta.Meta(),
           host = '127.0.0.1')

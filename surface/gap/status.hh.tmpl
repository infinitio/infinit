#ifndef SURFACE_GAP_STATUS_HH
# define SURFACE_GAP_STATUS_HH

typedef enum
{
  gap_ok = 1,
  gap_error = 0,
  gap_network_error = -2,
  gap_internal_error = -3,
  gap_no_device_error = -4,
  gap_wrong_passport = -5,
  gap_no_file = -6,
  gap_file_not_found = -7,
  gap_api_error = -10,
  <%! from infinit.oracles.meta.error import errors %>
  %for name, (code, comment) in errors.items():
    gap_${name} = ${code},
  %endfor
} gap_Status;

  //- Transaction -------------------------------------------------------------
typedef enum
{
  <%! from infinit.oracles.transaction import statuses %>
  %for name, value in statuses.items():
    gap_transaction_status_${name} = ${value},
  %endfor
} gap_TransactionStatus;

#endif
